def twoSum(nums,t):
    n = len(nums)
    low = 0
    high = n - 1
    while low < high:#TC - O(logN)
        s = nums[low] + nums[high]
        if s == t:
            return True
        elif s > t:
            high -= 1
        else:
            low += 1
    return False
print(twoSum([1,2,3,1,4],7))
